
@model IOTAmHackCore.Models.RoomViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <div class="tiles-grid tiles-group size-3" data-group-title="" style="margin-right: auto; margin-left: auto; float: none">
    @foreach (var item in Model.Rooms)
    {
        <div data-role="tile" data-size="medium" class=@(item.IsFree ? "bg-green" : "bg-red")>
            <span class="branding-bar">@item.Key</span>
            <span class="badge-bottom">@item.Name</span>
        </div>
    }
    </div>
